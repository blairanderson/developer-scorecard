<div class="tabs-container">
  <div class="tab-list">
    <div class="home">
      <a href="">home</a>
    </div>
    <div class="profile">
      <a href="">profile</a>
    </div>
    <div class="messages active">
      <a href="">messages</a>
    </div>
  </div>
  <div class="tabs-content">
    connected users
    {{#each connected_users}}
      <div class="user-name">
        {{#link-to 'user' username}} {{name}} - {{username}} {{/link-to}}
      </div>
    {{/each}}


    repositories
    <ul>
      {{#each repositories}}
        <li>{{count}} - {{repo}}</li>
      {{/each}}
    </ul>


    similar users
    <ul>
    {{#each similar_users}}
      <li>
        {{#link-to 'user' username}} {{name}} - {{username}} {{/link-to}}
      </li>
    {{/each}}
    </ul>
  </div>

</div>
